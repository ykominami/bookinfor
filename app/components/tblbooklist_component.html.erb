<div>
    <table>
        <thead>
            <tr>
                <% @header.each do |col| %>
                <th scope="col"><%= col %></th>
                <% end %>
            </tr>
        </thead>
        <tbody>
            <% @body.each do |row| %>
            <tr>
                <% row.each do |col| %>
                <td>
                    <%= col %>
                </td>
                <% end %>
            </tr>
            <% end %>
        </tbody>
        <tfoot>
        </tfoot>
    </table>
</div>
